#include <avr/pgmspace.h>

#define PIN0 11
#define PIN1 12
#define SWITCH_PIN0 0
#define SWITCH_PIN1 1

// the setup function runs once when you press reset or power the board
// Wiring:
//  - audio output: between PIN0 and PIN1
//  - switch: between SWITCH_PIN0 and SWITCH_PIN1
void setup() {
  pinMode(PIN0, OUTPUT);
  pinMode(PIN1, OUTPUT);
  pinMode(SWITCH_PIN0, INPUT_PULLUP); // connect switch between these two pins
  pinMode(SWITCH_PIN1, OUTPUT);
  digitalWrite(SWITCH_PIN1, LOW); // used as GND pin that is physically closer to pin 0 on the connector
}

// because our bit-level sampling rate matters for noise, we use direct HW access for speed
#if 1  // fast version using direct hardware access
#define PIN_PORT (( (PIN0)<8 ? PORTD : PORTB   )) // hoping the compiler folds that a compile-time
#define PIN0_BIT  (( (PIN0)<8 ? (PIN0) : (PIN0-8) ))
#define PIN1_BIT  (( (PIN1)<8 ? (PIN1) : (PIN1-8) ))
#define PIN0_MASK (1 << (PIN0_BIT))
#define PIN1_MASK (1 << (PIN1_BIT))
#define writeBit(bit255) (PIN_PORT = ((PIN0_MASK | PIN1_MASK) & (bit255)) ^ PIN1_MASK) // bit255 = 0 or 255
#else  // slow version calling digitalWrite()
#define writeBit(bit255) (digitalWrite(PIN0, (bit255), digitalWrite(PIN1, (~bit255)))
#endif

// plays an audio sample through a single-bit port using sigma-delta coding
void playSampleBuffer(const char* data/*in PROGMEM*/, unsigned int length, int sampleRate) {
  // because interrupts cause audio artifacts, but micros() does not work with noInterrupt(),
  // we must time this manually. The following variable is the length of one loop iteration,
  // in tenth of microseconds (100 ns). It has been manually tuned (approximately)
  noInterrupts();
again:
  char outBit = 0;
  for(; digitalRead(SWITCH_PIN0) == HIGH;) {
    writeBit(outBit);                          // send to digital port
    outBit = ~outBit;
  }
  const unsigned char tenthMicrosPerLoop = 23;
  const unsigned int  tenthMicrosPerPeriod = 10000000 / sampleRate; // sample period in tenth of micro seconds
  // note: if tenthMicrosPerLoop * tenthMicrosPerPeriod exceeds 16 bits, then type should be 'long'

  int tenthMicros = 0;  // how many tenth of microseconds have elapsed within this sample period?
  char residual = 0;    // current aggregate quantization error
  const char* end = data + length;
  for (const char* p = data; p < end; tenthMicros += tenthMicrosPerLoop) {
    // note: the loop body was written without conditionals in order to ensure constant timing

    // produce one bit
    char sampleValue = (char)pgm_read_byte(p); // sample value to output (stored in EEPROM=PROGMEM)
    sampleValue += residual;                   // incorporate error from last sample (note: we need 9 bits here, hence 'int')
    char outBit = -(sampleValue < 0);          // quantize to 1 bit (-1 if sampleValue < 0 else 0). Negative because it is 1 instr. less
    writeBit(outBit);                          // send to digital port
    char outValue = 63 - (126 & outBit);       // convert bit to actual value (63 if sampleValue >= 0 else -63) (note: avoid multiplication or conditional)
    residual = (char)sampleValue - outValue;   // compute quantization error of this step (note: guaranteed to fit into 8 bits)

    // advance the time index if the time duration of a sample period has elapsed
    //unsigned int advance = (tenthMicros > tenthMicrosPerPeriod);
    unsigned int advanceNeg = ((int)(tenthMicrosPerPeriod - tenthMicros)) >> (int)15; // 0 if tenthMicros < tenthMicrosPerPeriod else -1
    unsigned int subtractTenthMicros = tenthMicrosPerPeriod & advanceNeg; // -1 = 0xffff
    tenthMicros -= subtractTenthMicros;
    p -= advanceNeg;
  }
  goto again;
}

